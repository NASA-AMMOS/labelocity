                    #getPds4DataTypeFieldBinaryCheckLabel( $columnNode   $columnNode.DATA_TYPE   $columnNode.BYTES )
                #end
                
                #if ( $columnNode.BYTES )
                    <field_length unit="byte">$!columnNode.BYTES</field_length>
                #end

                <unit>$!columnNode.UNIT</unit>

                #if ( $columnNode.DESCRIPTION )                
