#set ( $m20_gtp_val = "Data Product" )
#elseif ($tp_raw.equals("SFDU") )
#set ( $m20_gtp_val = "SFDU" )
#elseif ($tp_raw.equalsIgnoreCase("Team-generated Image") )
#set ( $m20_gtp_val = "Team-generated Image" )
#end
${m20_gtp_val}##
#end

